{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "[github-report-cli] Build and run project",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/github-report-cli"
			},
			"command": "yarn",
			"args": [
				"run",
				"start"
			],
			"group": "build",
			"problemMatcher": [
				"$tsc"
			]
		},
		{
			"label": "[graphviz-d3] Build and run project",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/graphviz-d3"
			},
			"command": "yarn",
			"args": [
				"run",
				"serve"
			],
			"group": "build",
			"problemMatcher": [
				"$tsc"
			]
		}
	]
}